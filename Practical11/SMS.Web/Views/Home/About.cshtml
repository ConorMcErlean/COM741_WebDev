@model AboutViewModel

<div class="card">
  <img class="card-img-top img-fluid" src="~/sms.jpeg" alt="Student Management System">
  <div class="card-body">
    <h5 class="card-title">@Model.Title</h5>
    <p>@Model.Message</p>
    <p>We were formed on @Model.Formed.ToShortDateString()</p>
    <p>And are now operating for @Model.Days days</p>
    <p class="small"><em>Current Date-time @Model.Now</em></p>
  </div>
</div>


